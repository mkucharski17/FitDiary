<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.fitdiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".Day.DietDay.Views.DietDayActivity"
            android:label="Diet Day"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".Day.TrainingDay.Views.TrainingDayActivity"
            android:label="Training Day"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".Views.ChooseActivity"
            android:label="Choose"/>
        <activity android:name=".Signing.Views.SigningInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Day.Views.DaysListActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".Signing.Views.RegistrationActivity"
            android:label="Registration"
            tools:ignore="InnerclassSeparator" />
    </application>

</manifest>